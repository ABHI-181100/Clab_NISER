# -----------question_2--solving linear equation---------------

from mylib import *

E = read_matrix().read_matrix1("t8.txt")
F = read_matrix().read_matrix1("t10.txt")

A_rearranged = diadominant().dia_dominant(E)
print("diagonally dominant matrix used here :")
for row in A_rearranged:
    print(row)

print(GaussSeidel().gauss_seidel_iter(A_rearranged,F),"value of x1,x2,x3,x4,x5,x6(from gauss_seidel)")
print(Jacobi1().jacobi_iter(A_rearranged,F),"value of x1,x2,x3,x4,x5,x6(from jacobi)")



