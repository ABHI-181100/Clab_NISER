# -----------question_1-----------------

from mylib import *

A = read_matrix().read_matrix1("t7.txt")
B = read_matrix().read_matrix1("t9.txt")

n= len(A)
def symm(A): #checking symmetricity
    A_symm = [[0 for _ in range(n)] for _ in range(n)]

    for i in range(n):
        for j in range(n):
            A_symm[i][j] = A[j][i]
    if A_symm == A:
        return "A is symmetric matrix"

print(symm(A))
print(cholesky_decom().cholesky_solve(A,B),"value of x1,x2,x3,x4,x5,x6")


# ---------using gauss_seidel-method------------------

C = read_matrix().read_matrix1("t7.txt")
D = read_matrix().read_matrix1("t9.txt")

print(GaussSeidel().gauss_seidel_iter(C,D),"value of x1,x2,x3,x4,x5,x6")



